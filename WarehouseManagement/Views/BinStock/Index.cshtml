@model IEnumerable<WarehouseManagement.DAL.Bin_Stock>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.quantity)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.stock_in_date)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.stock_out_date)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Bin.name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Stock.name)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.quantity)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.stock_in_date)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.stock_out_date)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Bin.name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Stock.name)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.id }) |
            @Html.ActionLink("Details", "Details", new { id=item.id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.id })
        </td>
    </tr>
}

</table>
